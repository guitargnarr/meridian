{
  "crons": [
    {
      "path": "/api/cron/daily-report",
      "schedule": "0 14 * * *"
    }
  ],
  "redirects": [
    {
      "source": "/:path*",
      "has": [{ "type": "host", "value": "phishguard-ui.vercel.app" }],
      "destination": "https://phishguard.projectlavos.com/:path*",
      "permanent": true
    }
  ]
}
